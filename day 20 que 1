#include <stdio.h>
int main() {
    int n, digit, product = 1, found = 0;
    printf("Enter a number: ");
    scanf("%d", &n);
    while (n > 0) {
        digit = n % 10;
        if (digit % 2 != 0) {
            product *= digit;
            found = 1;
        }
        n /= 10;
    }
    if (found)
        printf("Product of odd digits = %d\n", product);
    else
        printf("No odd digits found.\n");
    return 0;
}
