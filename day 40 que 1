#include <stdio.h>

int main() {
    int n, a[10][10];
    int i, j;

    printf("Enter size of square matrix: ");
    scanf("%d", &n);

    printf("Enter matrix elements:\n");
    for(i = 0; i < n; i++)
        for(j = 0; j < n; j++)
            scanf("%d", &a[i][j]);

    printf("Diagonal traversal:\n");
    for(i = 0; i < n; i++) {
        for(j = 0; j < n; j++) {
            if(i == j)
                printf("%d ", a[i][j]);
        }
    }

    return 0;
}
