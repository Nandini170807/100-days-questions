#include <stdio.h>
#include <string.h>

int main() {
    char s[] = "abcabcbb";
    int n = strlen(s);
    int maxLen = 0;

    for (int i = 0; i < n; i++) {
        int freq[256] = {0};
        for (int j = i; j < n; j++) {
            if (freq[(int)s[j]] == 1)
                break;
            freq[(int)s[j]] = 1;
            int len = j - i + 1;
            if (len > maxLen)
                maxLen = len;
        }
    }

    printf("Length of longest substring: %d\n", maxLen);
    return 0;
}
