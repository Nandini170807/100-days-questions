#include <stdio.h>

int main() {
    int nums[] = {2, 2, 1, 1, 1, 2, 2};
    int n = sizeof(nums)/sizeof(nums[0]);

    // Boyer-Moore Voting
    int count = 0, candidate = -1;
    for (int i = 0; i < n; i++) {
        if (count == 0)
            candidate = nums[i];
        count += (nums[i] == candidate) ? 1 : -1;
    }

    // Verify majority
    count = 0;
    for (int i = 0; i < n; i++) {
        if (nums[i] == candidate)
            count++;
    }

    if (count > n / 2)
        printf("Majority element: %d\n", candidate);
    else
        printf("Majority element: -1\n");

    return 0;
}
