#include <stdio.h>

int main() {
    int i, j, n, k, isDistinct = 1;
    int a[10][10];

    printf("Enter size of square matrix: ");
    scanf("%d", &n);

    printf("Enter matrix elements:\n");
    for(i = 0; i < n; i++)
        for(j = 0; j < n; j++)
            scanf("%d", &a[i][j]);

    for(i = 0; i < n; i++) {
        for(k = i + 1; k < n; k++) {
            if(a[i][i] == a[k][k]) {
                isDistinct = 0;
                break;
            }
        }
    }

    if(isDistinct)
        printf("Diagonal elements are distinct.\n");
    else
        printf("Diagonal elements are not distinct.\n");

    return 0;
}
