#include <stdio.h>

int main() {
    char str1[] = "listen";
    char str2[] = "silent";
    int count1[26] = {0}, count2[26] = {0};
    int i, flag = 0;

    // Count frequency of each letter in str1 and str2
    for (i = 0; str1[i] != '\0'; i++) {
        if (str1[i] >= 'a' && str1[i] <= 'z')
            count1[str1[i] - 'a']++;
    }

    for (i = 0; str2[i] != '\0'; i++) {
        if (str2[i] >= 'a' && str2[i] <= 'z')
            count2[str2[i] - 'a']++;
    }

    // Compare frequencies
    for (i = 0; i < 26; i++) {
        if (count1[i] != count2[i]) {
            flag = 1;
            break;
        }
    }

    if (flag == 0)
        printf("Strings are anagrams.\n");
    else
        printf("Strings are NOT anagrams.\n");

    return 0;
}
