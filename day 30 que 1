#include <stdio.h>

int main() {
    int n, i, even = 0, odd = 0, arr[100];

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        if(arr[i] % 2 == 0)
            even++;
        else
            odd++;
    }

    printf("Even numbers = %d\nOdd numbers = %d\n", even, odd);
    return 0;
}
